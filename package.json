{
  "name": "open-coworker",
  "version": "0.0.1",
  "private": true,
  "description": "Cross-platform desktop agentic application",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn workspace ui dev",
    "dev:agent": "yarn workspace agent dev",
    "build": "yarn workspace ui build",
    "start": "yarn workspace ui start",
    "lint": "yarn workspace ui lint && yarn workspace agent lint",
    "clean": "yarn workspaces run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "electron": "^39.2.7",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  }
}
